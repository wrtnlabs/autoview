{
  "schema": {
    "$ref": "#/$defs/repository-advisory"
  },
  "$defs": {
    "repository-advisory-update": {
      "type": "object",
      "properties": {
        "summary": {
          "description": "A short summary of the advisory.",
          "type": "string",
          "maxLength": 1024
        },
        "description": {
          "description": "A detailed description of what the advisory impacts.",
          "type": "string",
          "maxLength": 65535
        },
        "cve_id": {
          "description": "The Common Vulnerabilities and Exposures (CVE) ID.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerabilities": {
          "description": "A product affected by the vulnerability detailed in a repository security advisory.",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "package": {
                "description": "The name of the package affected by the vulnerability.",
                "type": "object",
                "properties": {
                  "ecosystem": {
                    "$ref": "#/$defs/security-advisory-ecosystems"
                  },
                  "name": {
                    "description": "The unique package name within its ecosystem.",
                    "oneOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "required": [
                  "ecosystem"
                ]
              },
              "vulnerable_version_range": {
                "description": "The range of the package versions affected by the vulnerability.",
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "patched_versions": {
                "description": "The package version(s) that resolve the vulnerability.",
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ]
              },
              "vulnerable_functions": {
                "description": "The functions in the package that are affected.",
                "oneOf": [
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  {
                    "type": "null"
                  }
                ]
              }
            },
            "required": [
              "package"
            ]
          }
        },
        "cwe_ids": {
          "description": "A list of Common Weakness Enumeration (CWE) IDs.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "credits": {
          "description": "A list of users receiving credit for their participation in the security advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "login": {
                    "description": "The username of the user credited.",
                    "type": "string"
                  },
                  "type": {
                    "$ref": "#/$defs/security-advisory-credit-types"
                  }
                },
                "required": [
                  "login",
                  "type"
                ]
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "severity": {
          "description": "The severity of the advisory. You must choose between setting this field or `cvss_vector_string`.",
          "oneOf": [
            {
              "const": "critical"
            },
            {
              "const": "high"
            },
            {
              "const": "medium"
            },
            {
              "const": "low"
            },
            {
              "type": "null"
            }
          ]
        },
        "cvss_vector_string": {
          "description": "The CVSS vector that calculates the severity of the advisory. You must choose between setting this field or `severity`.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "state": {
          "description": "The state of the advisory.",
          "oneOf": [
            {
              "const": "published"
            },
            {
              "const": "closed"
            },
            {
              "const": "draft"
            }
          ]
        },
        "collaborating_users": {
          "description": "A list of usernames who have been granted write access to the advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "collaborating_teams": {
          "description": "A list of team slugs which have been granted write access to the advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": []
    },
    "security-advisory-ecosystems": {
      "description": "The package's language or package management ecosystem.",
      "oneOf": [
        {
          "const": "rubygems"
        },
        {
          "const": "npm"
        },
        {
          "const": "pip"
        },
        {
          "const": "maven"
        },
        {
          "const": "nuget"
        },
        {
          "const": "composer"
        },
        {
          "const": "go"
        },
        {
          "const": "rust"
        },
        {
          "const": "erlang"
        },
        {
          "const": "actions"
        },
        {
          "const": "pub"
        },
        {
          "const": "other"
        },
        {
          "const": "swift"
        }
      ]
    },
    "security-advisory-credit-types": {
      "description": "The type of credit the user is receiving.",
      "oneOf": [
        {
          "const": "analyst"
        },
        {
          "const": "finder"
        },
        {
          "const": "reporter"
        },
        {
          "const": "coordinator"
        },
        {
          "const": "remediation_developer"
        },
        {
          "const": "remediation_reviewer"
        },
        {
          "const": "remediation_verifier"
        },
        {
          "const": "tool"
        },
        {
          "const": "sponsor"
        },
        {
          "const": "other"
        }
      ]
    },
    "repository-advisory": {
      "description": "A repository security advisory.",
      "type": "object",
      "properties": {
        "ghsa_id": {
          "description": "The GitHub Security Advisory ID.",
          "type": "string",
          "readOnly": true
        },
        "cve_id": {
          "description": "The Common Vulnerabilities and Exposures (CVE) ID.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "url": {
          "description": "The API URL for the advisory.",
          "type": "string",
          "format": "uri",
          "readOnly": true
        },
        "html_url": {
          "description": "The URL for the advisory.",
          "type": "string",
          "format": "uri",
          "readOnly": true
        },
        "summary": {
          "description": "A short summary of the advisory.",
          "type": "string",
          "maxLength": 1024
        },
        "description": {
          "description": "A detailed description of what the advisory entails.",
          "oneOf": [
            {
              "type": "string",
              "maxLength": 65535
            },
            {
              "type": "null"
            }
          ]
        },
        "severity": {
          "description": "The severity of the advisory.",
          "oneOf": [
            {
              "const": "critical"
            },
            {
              "const": "high"
            },
            {
              "const": "medium"
            },
            {
              "const": "low"
            },
            {
              "type": "null"
            }
          ]
        },
        "author": {
          "description": "The author of the advisory.",
          "oneOf": [
            {
              "$ref": "#/$defs/simple-user"
            },
            {
              "type": "null"
            }
          ]
        },
        "publisher": {
          "description": "The publisher of the advisory.",
          "oneOf": [
            {
              "$ref": "#/$defs/simple-user"
            },
            {
              "type": "null"
            }
          ]
        },
        "identifiers": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "description": "The type of identifier.",
                "oneOf": [
                  {
                    "const": "CVE"
                  },
                  {
                    "const": "GHSA"
                  }
                ]
              },
              "value": {
                "description": "The identifier value.",
                "type": "string"
              }
            },
            "required": [
              "type",
              "value"
            ]
          },
          "readOnly": true
        },
        "state": {
          "description": "The state of the advisory.",
          "oneOf": [
            {
              "const": "published"
            },
            {
              "const": "closed"
            },
            {
              "const": "withdrawn"
            },
            {
              "const": "draft"
            },
            {
              "const": "triage"
            }
          ]
        },
        "created_at": {
          "description": "The date and time of when the advisory was created, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "updated_at": {
          "description": "The date and time of when the advisory was last updated, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "published_at": {
          "description": "The date and time of when the advisory was published, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "closed_at": {
          "description": "The date and time of when the advisory was closed, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "withdrawn_at": {
          "description": "The date and time of when the advisory was withdrawn, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "submission": {
          "oneOf": [
            {
              "type": "object",
              "readOnly": true,
              "properties": {
                "accepted": {
                  "description": "Whether a private vulnerability report was accepted by the repository's administrators.",
                  "type": "boolean",
                  "readOnly": true
                }
              },
              "required": [
                "accepted"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerabilities": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/$defs/repository-advisory-vulnerability"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "cvss": {
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "vector_string": {
                  "description": "The CVSS vector.",
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "score": {
                  "description": "The CVSS score.",
                  "oneOf": [
                    {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 10,
                      "readOnly": true
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "required": [
                "vector_string",
                "score"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "cvss_severities": {
          "$ref": "#/$defs/cvss-severities"
        },
        "cwes": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "cwe_id": {
                    "description": "The Common Weakness Enumeration (CWE) identifier.",
                    "type": "string"
                  },
                  "name": {
                    "description": "The name of the CWE.",
                    "type": "string",
                    "readOnly": true
                  }
                },
                "required": [
                  "cwe_id",
                  "name"
                ]
              },
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "cwe_ids": {
          "description": "A list of only the CWE IDs.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "credits": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "login": {
                    "description": "The username of the user credited.",
                    "type": "string"
                  },
                  "type": {
                    "$ref": "#/$defs/security-advisory-credit-types"
                  }
                },
                "required": []
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "credits_detailed": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/$defs/repository-advisory-credit"
              },
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "collaborating_users": {
          "description": "A list of users that collaborate on the advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/$defs/simple-user"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "collaborating_teams": {
          "description": "A list of teams that collaborate on the advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/$defs/team"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "private_fork": {
          "description": "A temporary private fork of the advisory's repository for collaborating on a fix.",
          "oneOf": [
            {
              "$ref": "#/$defs/simple-repository"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "ghsa_id",
        "cve_id",
        "url",
        "html_url",
        "summary",
        "description",
        "severity",
        "author",
        "publisher",
        "identifiers",
        "state",
        "created_at",
        "updated_at",
        "published_at",
        "closed_at",
        "withdrawn_at",
        "submission",
        "vulnerabilities",
        "cvss",
        "cwes",
        "cwe_ids",
        "credits",
        "credits_detailed",
        "collaborating_users",
        "collaborating_teams",
        "private_fork"
      ]
    },
    "simple-user": {
      "title": "Simple User",
      "description": "A GitHub user.",
      "type": "object",
      "properties": {
        "name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "email": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "login": {
          "example": "octocat",
          "type": "string"
        },
        "id": {
          "example": 1,
          "type": "integer",
          "format": "int64"
        },
        "node_id": {
          "example": "MDQ6VXNlcjE=",
          "type": "string"
        },
        "avatar_url": {
          "example": "https://github.com/images/error/octocat_happy.gif",
          "type": "string",
          "format": "uri"
        },
        "gravatar_id": {
          "example": "41d064eb2195891e12d0413f63227ea7",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "url": {
          "example": "https://api.github.com/users/octocat",
          "type": "string",
          "format": "uri"
        },
        "html_url": {
          "example": "https://github.com/octocat",
          "type": "string",
          "format": "uri"
        },
        "followers_url": {
          "example": "https://api.github.com/users/octocat/followers",
          "type": "string",
          "format": "uri"
        },
        "following_url": {
          "example": "https://api.github.com/users/octocat/following{/other_user}",
          "type": "string"
        },
        "gists_url": {
          "example": "https://api.github.com/users/octocat/gists{/gist_id}",
          "type": "string"
        },
        "starred_url": {
          "example": "https://api.github.com/users/octocat/starred{/owner}{/repo}",
          "type": "string"
        },
        "subscriptions_url": {
          "example": "https://api.github.com/users/octocat/subscriptions",
          "type": "string",
          "format": "uri"
        },
        "organizations_url": {
          "example": "https://api.github.com/users/octocat/orgs",
          "type": "string",
          "format": "uri"
        },
        "repos_url": {
          "example": "https://api.github.com/users/octocat/repos",
          "type": "string",
          "format": "uri"
        },
        "events_url": {
          "example": "https://api.github.com/users/octocat/events{/privacy}",
          "type": "string"
        },
        "received_events_url": {
          "example": "https://api.github.com/users/octocat/received_events",
          "type": "string",
          "format": "uri"
        },
        "type": {
          "example": "User",
          "type": "string"
        },
        "site_admin": {
          "type": "boolean"
        },
        "starred_at": {
          "example": "\"2020-07-09T00:17:55Z\"",
          "type": "string"
        },
        "user_view_type": {
          "example": "public",
          "type": "string"
        }
      },
      "required": [
        "avatar_url",
        "events_url",
        "followers_url",
        "following_url",
        "gists_url",
        "gravatar_id",
        "html_url",
        "id",
        "node_id",
        "login",
        "organizations_url",
        "received_events_url",
        "repos_url",
        "site_admin",
        "starred_url",
        "subscriptions_url",
        "type",
        "url"
      ]
    },
    "repository-advisory-vulnerability": {
      "description": "A product affected by the vulnerability detailed in a repository security advisory.",
      "type": "object",
      "properties": {
        "package": {
          "description": "The name of the package affected by the vulnerability.",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "ecosystem": {
                  "$ref": "#/$defs/security-advisory-ecosystems"
                },
                "name": {
                  "description": "The unique package name within its ecosystem.",
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "required": [
                "ecosystem",
                "name"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerable_version_range": {
          "description": "The range of the package versions affected by the vulnerability.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "patched_versions": {
          "description": "The package version(s) that resolve the vulnerability.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerable_functions": {
          "description": "The functions in the package that are affected.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "package",
        "vulnerable_version_range",
        "patched_versions",
        "vulnerable_functions"
      ]
    },
    "cvss-severities": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "cvss_v3": {
              "oneOf": [
                {
                  "type": "object",
                  "properties": {
                    "vector_string": {
                      "description": "The CVSS 3 vector string.",
                      "oneOf": [
                        {
                          "type": "string"
                        },
                        {
                          "type": "null"
                        }
                      ]
                    },
                    "score": {
                      "description": "The CVSS 3 score.",
                      "oneOf": [
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 10,
                          "readOnly": true
                        },
                        {
                          "type": "null"
                        }
                      ]
                    }
                  },
                  "required": [
                    "vector_string",
                    "score"
                  ]
                },
                {
                  "type": "null"
                }
              ]
            },
            "cvss_v4": {
              "oneOf": [
                {
                  "type": "object",
                  "properties": {
                    "vector_string": {
                      "description": "The CVSS 4 vector string.",
                      "oneOf": [
                        {
                          "type": "string"
                        },
                        {
                          "type": "null"
                        }
                      ]
                    },
                    "score": {
                      "description": "The CVSS 4 score.",
                      "oneOf": [
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 10,
                          "readOnly": true
                        },
                        {
                          "type": "null"
                        }
                      ]
                    }
                  },
                  "required": [
                    "vector_string",
                    "score"
                  ]
                },
                {
                  "type": "null"
                }
              ]
            }
          },
          "required": []
        },
        {
          "type": "null"
        }
      ]
    },
    "repository-advisory-credit": {
      "description": "A credit given to a user for a repository security advisory.",
      "type": "object",
      "properties": {
        "user": {
          "$ref": "#/$defs/simple-user"
        },
        "type": {
          "$ref": "#/$defs/security-advisory-credit-types"
        },
        "state": {
          "description": "The state of the user's acceptance of the credit.",
          "oneOf": [
            {
              "const": "accepted"
            },
            {
              "const": "declined"
            },
            {
              "const": "pending"
            }
          ]
        }
      },
      "required": [
        "user",
        "type",
        "state"
      ]
    },
    "team": {
      "title": "Team",
      "description": "Groups of organization members that gives permissions on specified repositories.",
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "node_id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        },
        "description": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "privacy": {
          "type": "string"
        },
        "notification_setting": {
          "type": "string"
        },
        "permission": {
          "type": "string"
