{
  "schema": {
    "$ref": "#/$defs/org-hook"
  },
  "$defs": {
    "IApiOrgsHooks.PostBody": {
      "type": "object",
      "properties": {
        "name": {
          "description": "Must be passed as \"web\".",
          "type": "string"
        },
        "config": {
          "description": "Key/value pairs to provide settings for this webhook.",
          "type": "object",
          "properties": {
            "url": {
              "$ref": "#/$defs/webhook-config-url"
            },
            "content_type": {
              "$ref": "#/$defs/webhook-config-content-type"
            },
            "secret": {
              "$ref": "#/$defs/webhook-config-secret"
            },
            "insecure_ssl": {
              "$ref": "#/$defs/webhook-config-insecure-ssl"
            },
            "username": {
              "example": "\"kdaigle\"",
              "type": "string"
            },
            "password": {
              "example": "\"password\"",
              "type": "string"
            }
          },
          "required": [
            "url"
          ]
        },
        "events": {
          "description": "Determines what [events](https://docs.github.com/webhooks/event-payloads) the hook is triggered for. Set to `[\"*\"]` to receive all possible events.",
          "type": "array",
          "default": [
            "push"
          ],
          "items": {
            "type": "string"
          }
        },
        "active": {
          "description": "Determines if notifications are sent when the webhook is triggered. Set to `true` to send notifications.",
          "type": "boolean",
          "default": true
        }
      },
      "required": [
        "name",
        "config"
      ]
    },
    "webhook-config-url": {
      "description": "The URL to which the payloads will be delivered.",
      "example": "https://example.com/webhook",
      "type": "string",
      "format": "uri"
    },
    "webhook-config-content-type": {
      "description": "The media type used to serialize the payloads. Supported values include `json` and `form`. The default is `form`.",
      "example": "\"json\"",
      "type": "string"
    },
    "webhook-config-secret": {
      "description": "If provided, the `secret` will be used as the `key` to generate the HMAC hex digest value for [delivery signature headers](https://docs.github.com/webhooks/event-payloads/#delivery-headers).",
      "example": "\"********\"",
      "type": "string"
    },
    "webhook-config-insecure-ssl": {
      "oneOf": [
        {
          "type": "string",
          "description": "Determines whether the SSL certificate of the host for `url` will be verified when delivering payloads. Supported values include `0` (verification is performed) and `1` (verification is not performed). The default is `0`. **We strongly recommend not setting this to `1` as you are subject to man-in-the-middle and other attacks.**",
          "example": "\"0\""
        },
        {
          "type": "number"
        }
      ]
    },
    "org-hook": {
      "title": "Org Hook",
      "description": "Org Hook",
      "type": "object",
      "properties": {
        "id": {
          "example": 1,
          "type": "integer"
        },
        "url": {
          "example": "https://api.github.com/orgs/octocat/hooks/1",
          "type": "string",
          "format": "uri"
        },
        "ping_url": {
          "example": "https://api.github.com/orgs/octocat/hooks/1/pings",
          "type": "string",
          "format": "uri"
        },
        "deliveries_url": {
          "example": "https://api.github.com/orgs/octocat/hooks/1/deliveries",
          "type": "string",
          "format": "uri"
        },
        "name": {
          "example": "web",
          "type": "string"
        },
        "events": {
          "example": [
            "push",
            "pull_request"
          ],
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "active": {
          "example": true,
          "type": "boolean"
        },
        "config": {
          "type": "object",
          "properties": {
            "url": {
              "example": "\"http://example.com/2\"",
              "type": "string"
            },
            "insecure_ssl": {
              "example": "\"0\"",
              "type": "string"
            },
            "content_type": {
              "example": "\"form\"",
              "type": "string"
            },
            "secret": {
              "example": "\"********\"",
              "type": "string"
            }
          },
          "required": []
        },
        "updated_at": {
          "example": "2011-09-06T20:39:23Z",
          "type": "string",
          "format": "date-time"
        },
        "created_at": {
          "example": "2011-09-06T17:26:27Z",
          "type": "string",
          "format": "date-time"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "url",
        "type",
        "name",
        "active",
        "events",
        "config",
        "ping_url",
        "created_at",
        "updated_at"
      ]
    }
  }
}