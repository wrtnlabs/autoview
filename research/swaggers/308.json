{
  "schema": {
    "type": "array",
    "items": {
      "$ref": "#/$defs/global-advisory"
    }
  },
  "$defs": {
    "IApiAdvisories.GetQuery": {
      "type": "object",
      "properties": {
        "ghsa_id": {
          "description": "If specified, only advisories with this GHSA (GitHub Security Advisory) identifier will be returned.",
          "type": "string"
        },
        "cve_id": {
          "description": "If specified, only advisories with this CVE (Common Vulnerabilities and Exposures) identifier will be returned.",
          "type": "string"
        },
        "is_withdrawn": {
          "description": "Whether to only return advisories that have been withdrawn.",
          "type": "boolean"
        },
        "published": {
          "description": "If specified, only return advisories that were published on a date or date range.\n\nFor more information on the syntax of the date range, see \"[Understanding the search syntax](https://docs.github.com/search-github/getting-started-with-searching-on-github/understanding-the-search-syntax#query-for-dates).\"",
          "type": "string"
        },
        "updated": {
          "description": "If specified, only return advisories that were updated on a date or date range.\n\nFor more information on the syntax of the date range, see \"[Understanding the search syntax](https://docs.github.com/search-github/getting-started-with-searching-on-github/understanding-the-search-syntax#query-for-dates).\"",
          "type": "string"
        },
        "modified": {
          "description": "If specified, only show advisories that were updated or published on a date or date range.\n\nFor more information on the syntax of the date range, see \"[Understanding the search syntax](https://docs.github.com/search-github/getting-started-with-searching-on-github/understanding-the-search-syntax#query-for-dates).\"",
          "type": "string"
        },
        "epss_percentage": {
          "description": "If specified, only return advisories that have an EPSS percentage score that matches the provided value.\nThe EPSS percentage represents the likelihood of a CVE being exploited.",
          "type": "string"
        },
        "epss_percentile": {
          "description": "If specified, only return advisories that have an EPSS percentile score that matches the provided value.\nThe EPSS percentile represents the relative rank of the CVE's likelihood of being exploited compared to other CVEs.",
          "type": "string"
        },
        "per_page": {
          "description": "The number of results per page (max 100). For more information, see \"[Using pagination in the REST API](https://docs.github.com/rest/using-the-rest-api/using-pagination-in-the-rest-api).\"",
          "type": "integer",
          "minimum": 1,
          "maximum": 100,
          "default": 30
        }
      },
      "required": []
    },
    "global-advisory": {
      "description": "A GitHub Security Advisory.",
      "type": "object",
      "properties": {
        "ghsa_id": {
          "description": "The GitHub Security Advisory ID.",
          "type": "string",
          "readOnly": true
        },
        "cve_id": {
          "description": "The Common Vulnerabilities and Exposures (CVE) ID.",
          "oneOf": [
            {
              "type": "string",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "url": {
          "description": "The API URL for the advisory.",
          "type": "string",
          "readOnly": true
        },
        "html_url": {
          "description": "The URL for the advisory.",
          "type": "string",
          "format": "uri",
          "readOnly": true
        },
        "repository_advisory_url": {
          "description": "The API URL for the repository advisory.",
          "oneOf": [
            {
              "type": "string",
              "format": "uri",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "summary": {
          "description": "A short summary of the advisory.",
          "type": "string",
          "maxLength": 1024
        },
        "description": {
          "description": "A detailed description of what the advisory entails.",
          "oneOf": [
            {
              "type": "string",
              "maxLength": 65535
            },
            {
              "type": "null"
            }
          ]
        },
        "type": {
          "description": "The type of advisory.",
          "oneOf": [
            {
              "const": "reviewed"
            },
            {
              "const": "unreviewed"
            },
            {
              "const": "malware"
            }
          ]
        },
        "severity": {
          "description": "The severity of the advisory.",
          "oneOf": [
            {
              "const": "critical"
            },
            {
              "const": "high"
            },
            {
              "const": "medium"
            },
            {
              "const": "low"
            },
            {
              "const": "unknown"
            }
          ]
        },
        "source_code_location": {
          "description": "The URL of the advisory's source code.",
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
        },
        "identifiers": {
          "oneOf": [
            {
              "type": "array",
              "readOnly": true,
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "description": "The type of identifier.",
                    "oneOf": [
                      {
                        "const": "CVE"
                      },
                      {
                        "const": "GHSA"
                      }
                    ]
                  },
                  "value": {
                    "description": "The identifier value.",
                    "type": "string"
                  }
                },
                "required": [
                  "type",
                  "value"
                ]
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "references": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "description": "URLs with more information regarding the advisory.",
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "published_at": {
          "description": "The date and time of when the advisory was published, in ISO 8601 format.",
          "type": "string",
          "format": "date-time",
          "readOnly": true
        },
        "updated_at": {
          "description": "The date and time of when the advisory was last updated, in ISO 8601 format.",
          "type": "string",
          "format": "date-time",
          "readOnly": true
        },
        "github_reviewed_at": {
          "description": "The date and time of when the advisory was reviewed by GitHub, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "nvd_published_at": {
          "description": "The date and time when the advisory was published in the National Vulnerability Database, in ISO 8601 format.\nThis field is only populated when the advisory is imported from the National Vulnerability Database.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "withdrawn_at": {
          "description": "The date and time of when the advisory was withdrawn, in ISO 8601 format.",
          "oneOf": [
            {
              "type": "string",
              "format": "date-time",
              "readOnly": true
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerabilities": {
          "description": "The products and respective version ranges affected by the advisory.",
          "oneOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/$defs/vulnerability"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "cvss": {
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "vector_string": {
                  "description": "The CVSS vector.",
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "score": {
                  "description": "The CVSS score.",
                  "oneOf": [
                    {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 10,
                      "readOnly": true
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "required": [
                "vector_string",
                "score"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "cvss_severities": {
          "$ref": "#/$defs/cvss-severities"
        },
        "epss": {
          "$ref": "#/$defs/security-advisory-epss"
        },
        "cwes": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "cwe_id": {
                    "description": "The Common Weakness Enumeration (CWE) identifier.",
                    "type": "string"
                  },
                  "name": {
                    "description": "The name of the CWE.",
                    "type": "string",
                    "readOnly": true
                  }
                },
                "required": [
                  "cwe_id",
                  "name"
                ]
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "credits": {
          "description": "The users who contributed to the advisory.",
          "oneOf": [
            {
              "type": "array",
              "readOnly": true,
              "items": {
                "type": "object",
                "properties": {
                  "user": {
                    "$ref": "#/$defs/simple-user"
                  },
                  "type": {
                    "$ref": "#/$defs/security-advisory-credit-types"
                  }
                },
                "required": [
                  "user",
                  "type"
                ]
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "ghsa_id",
        "cve_id",
        "url",
        "html_url",
        "repository_advisory_url",
        "summary",
        "description",
        "type",
        "severity",
        "source_code_location",
        "identifiers",
        "references",
        "published_at",
        "updated_at",
        "github_reviewed_at",
        "nvd_published_at",
        "withdrawn_at",
        "vulnerabilities",
        "cvss",
        "cwes",
        "credits"
      ]
    },
    "vulnerability": {
      "description": "A vulnerability describing the product and its affected versions within a GitHub Security Advisory.",
      "type": "object",
      "properties": {
        "package": {
          "description": "The name of the package affected by the vulnerability.",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "ecosystem": {
                  "$ref": "#/$defs/security-advisory-ecosystems"
                },
                "name": {
                  "description": "The unique package name within its ecosystem.",
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "required": [
                "ecosystem",
                "name"
              ]
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerable_version_range": {
          "description": "The range of the package versions affected by the vulnerability.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "first_patched_version": {
          "description": "The package version that resolves the vulnerability.",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "vulnerable_functions": {
          "description": "The functions in the package that are affected by the vulnerability.",
          "oneOf": [
            {
              "type": "array",
              "readOnly": true,
              "items": {
                "type": "string"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "package",
        "vulnerable_version_range",
        "first_patched_version",
        "vulnerable_functions"
      ]
    },
    "security-advisory-ecosystems": {
      "description": "The package's language or package management ecosystem.",
      "oneOf": [
        {
          "const": "rubygems"
        },
        {
          "const": "npm"
        },
        {
          "const": "pip"
        },
        {
          "const": "maven"
        },
        {
          "const": "nuget"
        },
        {
          "const": "composer"
        },
        {
          "const": "go"
        },
        {
          "const": "rust"
        },
        {
          "const": "erlang"
        },
        {
          "const": "actions"
        },
        {
          "const": "pub"
        },
        {
          "const": "other"
        },
        {
          "const": "swift"
        }
      ]
    },
    "cvss-severities": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "cvss_v3": {
              "oneOf": [
                {
                  "type": "object",
                  "properties": {
                    "vector_string": {
                      "description": "The CVSS 3 vector string.",
                      "oneOf": [
                        {
                          "type": "string"
                        },
                        {
                          "type": "null"
                        }
                      ]
                    },
                    "score": {
                      "description": "The CVSS 3 score.",
                      "oneOf": [
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 10,
                          "readOnly": true
                        },
                        {
                          "type": "null"
                        }
                      ]
                    }
                  },
                  "required": [
                    "vector_string",
                    "score"
                  ]
                },
                {
                  "type": "null"
                }
              ]
            },
            "cvss_v4": {
              "oneOf": [
                {
                  "type": "object",
                  "properties": {
                    "vector_string": {
                      "description": "The CVSS 4 vector string.",
                      "oneOf": [
                        {
                          "type": "string"
                        },
                        {
                          "type": "null"
                        }
                      ]
                    },
                    "score": {
                      "description": "The CVSS 4 score.",
                      "oneOf": [
                        {
                          "type": "number",
                          "minimum": 0,
                          "maximum": 10,
                          "readOnly": true
                        },
                        {
                          "type": "null"
                        }
                      ]
                    }
                  },
                  "required": [
                    "vector_string",
                    "score"
                  ]
                },
                {
                  "type": "null"
                }
              ]
            }
          },
          "required": []
        },
        {
          "type": "null"
        }
      ]
    },
    "security-advisory-epss": {
      "description": "The EPSS scores as calculated by the [Exploit Prediction Scoring System](https://www.first.org/epss).",
      "oneOf": [
        {
          "type": "object",
          "readOnly": true,
          "properties": {
            "percentage": {
              "type": "number",
              "minimum": 0,
              "maximum": 100
            },
            "percentile": {
              "type": "number",
              "minimum": 0,
              "maximum": 100
            }
          },
          "required": []
        },
        {
          "type": "null"
        }
      ]
    },
    "simple-user": {
      "title": "Simple User",
      "description": "A GitHub user.",
      "type": "object",
      "properties": {
        "name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "email": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "login": {
          "example": "octocat",
          "type": "string"
        },
        "id": {
          "example": 1,
          "type": "integer",
          "format": "int64"
        },
        "node_id": {
          "example": "MDQ6VXNlcjE=",
          "type": "string"
        },
        "avatar_url": {
          "example": "https://github.com/images/error/octocat_happy.gif",
          "type": "string",
          "format": "uri"
        },
        "gravatar_id": {
          "example": "41d064eb2195891e12d0413f63227ea7",
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "url": {
          "example": "https://api.github.com/users/octocat",
          "type": "string",
          "format": "uri"
        },
        "html_url": {
          "example": "https://github.com/octocat",
          "type": "string",
          "format": "uri"
        },
        "followers_url": {
          "example": "https://api.github.com/users/octocat/followers",
          "type": "string",
          "format": "uri"
        },
        "following_url": {
          "example": "https://api.github.com/users/octocat/following{/other_user}",
          "type": "string"
        },
        "gists_url": {
          "example": "https://api.github.com/users/octocat/gists{/gist_id}",
          "type": "string"
        },
        "starred_url": {
          "example": "https://api.github.com/users/octocat/starred{/owner}{/repo}",
          "type": "string"
        },
        "subscriptions_url": {
          "example": "https://api.github.com/users/octocat/subscriptions",
          "type": "string",
          "format": "uri"
        },
        "organizations_url": {
          "example": "https://api.github.com/users/octocat/orgs",
          "type": "string",
          "format": "uri"
        },
        "repos_url": {
          "example": "https://api.github.com/users/octocat/repos",
          "type": "string",
          "format": "uri"
        },
        "events_url": {
          "example": "https://api.github.com/users/octocat/events{/privacy}",
          "type": "string"
        },
        "received_events_url": {
          "example": "https://api.github.com/users/octocat/received_events",
          "type": "string",
          "format": "uri"
        },
        "type": {
          "example": "User",
          "type": "string"
        },
        "site_admin": {
          "type": "boolean"
        },
        "starred_at": {
          "example": "\"2020-07-09T00:17:55Z\"",
          "type": "string"
        },
        "user_view_type": {
          "example": "public",
          "type": "string"
        }
      },
      "required": [
        "avatar_url",
        "events_url",
        "followers_url",
        "following_url",
        "gists_url",
        "gravatar_id",
        "html_url",
        "id",
        "node_id",
        "login",
        "organizations_url",
        "received_events_url",
        "repos_url",
        "site_admin",
        "starred_url",
        "subscriptions_url",
        "type",
        "url"
      ]
    },
    "security-advisory-credit-types": {
      "description": "The type of credit the user is receiving.",
      "oneOf": [
        {
          "const": "analyst"
        },
        {
          "const": "finder"
        },
        {
          "const": "reporter"
        },
        {
          "const": "coordinator"
        },
        {
          "const": "remediation_developer"
        },
        {
          "const": "remediation_reviewer"
        },
        {
          "const": "remediation_verifier"
        },
        {
          "const": "tool"
        },
        {
          "const": "sponsor"
        },
        {
          "const": "other"
        }
      ]
    }
  }
}