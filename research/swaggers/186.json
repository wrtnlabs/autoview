{
  "schema": {
    "$ref": "#/$defs/legacy.open.v4.LegacyV4GroupView"
  },
  "$defs": {
    "legacy.open.v4.LegacyV4GroupView": {
      "type": "object",
      "properties": {
        "managers": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/legacy.v4.LegacyV4Manager"
          }
        },
        "onlines": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/legacy.v4.LegacyV4Online"
          }
        },
        "bookmark": {
          "$ref": "#/$defs/legacy.v4.LegacyV4ChatBookmark"
        },
        "session": {
          "$ref": "#/$defs/legacy.v4.LegacyV4ChatSession"
        },
        "group": {
          "$ref": "#/$defs/legacy.v4.LegacyV4Group"
        }
      },
      "required": []
    },
    "legacy.v4.LegacyV4Manager": {
      "required": [
        "email",
        "name",
        "role"
      ],
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "readOnly": true
        },
        "channelId": {
          "type": "string",
          "readOnly": true
        },
        "accountId": {
          "type": "string",
          "readOnly": true
        },
        "name": {
          "type": "string",
          "readOnly": true
        },
        "description": {
          "type": "string",
          "readOnly": true
        },
        "showDescriptionToFront": {
          "type": "boolean",
          "readOnly": true
        },
        "nameDescI18nMap": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/$defs/NameDesc"
          },
          "readOnly": true,
          "properties": {},
          "required": []
        },
        "profile": {
          "type": "object",
          "properties": {
            "empty": {
              "type": "boolean"
            }
          },
          "additionalProperties": {
            "type": "object",
            "properties": {},
            "required": []
          },
          "readOnly": true,
          "required": []
        },
        "email": {
          "type": "string",
          "readOnly": true
        },
        "showEmailToFront": {
          "type": "boolean",
          "readOnly": true
        },
        "mobileNumber": {
          "type": "string",
          "readOnly": true,
          "default": "+18004424000"
        },
        "showMobileNumberToFront": {
          "type": "boolean",
          "readOnly": true
        },
        "role": {
          "oneOf": [
            {
              "const": "owner"
            },
            {
              "const": "member"
            }
          ]
        },
        "removed": {
          "type": "boolean",
          "readOnly": true
        },
        "createdAt": {
          "example": 1744704932155,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "displayAsChannel": {
          "type": "boolean"
        },
        "defaultGroupWatch": {
          "oneOf": [
            {
              "const": "all"
            },
            {
              "const": "info"
            },
            {
              "const": "none"
            }
          ]
        },
        "defaultDirectChatWatch": {
          "oneOf": [
            {
              "const": "all"
            },
            {
              "const": "info"
            },
            {
              "const": "none"
            }
          ]
        },
        "defaultUserChatWatch": {
          "oneOf": [
            {
              "const": "all"
            },
            {
              "const": "info"
            },
            {
              "const": "none"
            }
          ]
        },
        "operatorScore": {
          "type": "number",
          "format": "float",
          "readOnly": true
        },
        "touchScore": {
          "type": "number",
          "format": "float",
          "readOnly": true
        },
        "avatar": {
          "$ref": "#/$defs/TinyFile"
        },
        "operatorEmailReminder": {
          "type": "boolean",
          "readOnly": true
        },
        "operator": {
          "type": "boolean"
        },
        "statusEmoji": {
          "type": "string",
          "readOnly": true
        },
        "statusText": {
          "type": "string",
          "readOnly": true
        },
        "statusClearAt": {
          "example": 1744704932173,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "managerId": {
          "type": "string"
        },
        "avatarUrl": {
          "type": "string"
        },
        "emailForFront": {
          "type": "string"
        },
        "mobileNumberForFront": {
          "type": "string",
          "default": "+18004424000"
        }
      }
    },
    "NameDesc": {
      "required": [
        "name"
      ],
      "type": "object",
      "properties": {
        "name": {
          "pattern": "^[^@#$%:/\\\\]+$",
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    },
    "TinyFile": {
      "required": [
        "bucket",
        "key"
      ],
      "type": "object",
      "properties": {
        "bucket": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "width": {
          "type": "integer",
          "format": "int32"
        },
        "height": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "legacy.v4.LegacyV4Online": {
      "type": "object",
      "properties": {
        "channelId": {
          "type": "string"
        },
        "personType": {
          "type": "string"
        },
        "personId": {
          "type": "string"
        },
        "id": {
          "type": "string"
        }
      },
      "required": []
    },
    "legacy.v4.LegacyV4ChatBookmark": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string",
          "readOnly": true
        },
        "chatId": {
          "type": "string",
          "readOnly": true
        },
        "chatKey": {
          "type": "string",
          "readOnly": true
        },
        "bookmarkKey": {
          "type": "string",
          "readOnly": true
        },
        "channelId": {
          "type": "string",
          "readOnly": true
        },
        "version": {
          "type": "integer",
          "format": "int64",
          "readOnly": true
        },
        "chatType": {
          "type": "string"
        },
        "personType": {
          "type": "string"
        },
        "personId": {
          "type": "string"
        }
      },
      "required": []
    },
    "legacy.v4.LegacyV4ChatSession": {
      "type": "object",
      "properties": {
        "key": {
          "type": "string",
          "readOnly": true
        },
        "chatId": {
          "type": "string",
          "readOnly": true
        },
        "chatKey": {
          "type": "string",
          "readOnly": true
        },
        "updatedKey": {
          "type": "string",
          "readOnly": true
        },
        "unreadKey": {
          "type": "string",
          "readOnly": true
        },
        "channelId": {
          "type": "string",
          "readOnly": true
        },
        "alert": {
          "type": "integer",
          "format": "int32",
          "readOnly": true
        },
        "unread": {
          "type": "integer",
          "format": "int32",
          "readOnly": true
        },
        "watch": {
          "oneOf": [
            {
              "const": "all"
            },
            {
              "const": "info"
            },
            {
              "const": "none"
            }
          ]
        },
        "readAt": {
          "example": 1744704933179,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "receivedAt": {
          "example": 1744704933179,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "postedAt": {
          "example": 1744704933179,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "updatedAt": {
          "example": 1744704933192,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "createdAt": {
          "example": 1744704933192,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "version": {
          "type": "integer",
          "format": "int64",
          "readOnly": true
        },
        "id": {
          "type": "string"
        },
        "chatType": {
          "type": "string"
        },
        "personType": {
          "type": "string"
        },
        "personId": {
          "type": "string"
        }
      },
      "required": []
    },
    "legacy.v4.LegacyV4Group": {
      "required": [
        "name",
        "scope"
      ],
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "readOnly": true
        },
        "channelId": {
          "type": "string",
          "readOnly": true
        },
        "name": {
          "pattern": "[\\p{L}\\p{N}-_()]+",
          "type": "string"
        },
        "scope": {
          "oneOf": [
            {
              "const": "all"
            },
            {
              "const": "public"
            },
            {
              "const": "private"
            }
          ]
        },
        "managerIds": {
          "maxItems": 2147483647,
          "minItems": 1,
          "uniqueItems": true,
          "type": "array",
          "readOnly": true,
          "items": {
            "type": "string"
          }
        },
        "icon": {
          "pattern": "\\S+",
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "createdAt": {
          "example": 1744704930266,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "updatedAt": {
          "example": 1744704930266,
          "type": "number",
          "format": "int64",
          "readOnly": true
        },
        "active": {
          "type": "boolean"
        }
      }
    }
  }
}